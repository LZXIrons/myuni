::-webkit-scrollbar {
  width: 0px;
  height: 0px;
}
::-webkit-scrollbar-track-piece {
  background-color: rgba(0, 0, 0, 0.2);
  -webkit-border-radius: 6px;
}
::-webkit-scrollbar-thumb:vertical {
  height: 5px;
  background-color: rgba(125, 125, 125, 0.7);
  -webkit-border-radius: 6px;
}
::-webkit-scrollbar-thumb:horizontal {
  width: 5px;
  background-color: rgba(125, 125, 125, 0.7);
  -webkit-border-radius: 6px;
}
.navbar-top {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100rpx;
  z-index: 999;
}
.uni-navbar__header {
  padding-top: var(--status-bar-height);
}
page, page {
  background-color: #f5f5f5;
}
uni-image, image {
  width: 100%;
  height: 100%;
}
uni-image ._img, image ._img {
    width: 100%;
    height: 100%;
}
view,
scroll-view,
swiper,
swiper-item,
cover-view,
cover-image,
icon,
text,
rich-text,
progress,
button,
checkbox,
form,
input,
label,
radio,
slider,
switch,
textarea,
navigator,
audio,
camera,
image,
video {
  font-size: 28rpx;
  box-sizing: border-box;
}
uni-button[type=default],
button[type=default] {
  color: #FA436A;
}
/* input 样式 */
.input-placeholder {
  color: #999999;
}
.placeholder {
  color: #999999;
}
.container {
  margin-top: 88rpx;
}

